<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="@Cancel">Cancel</MudButton>
        <MudButton OnClick="@Submit" Color="@Color" Variant="Variant.Filled">@ButtonText</MudButton>
    </DialogActions>
</MudDialog>